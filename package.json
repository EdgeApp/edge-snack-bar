{
  "name": "edge-snack-bar",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "configure": "configure",
    "build.web": "webpack --mode production",
    "build.server": "tsc -p tsconfig.server.json",
    "prepare": "husky install && npm-run-all clean -p build.*",
    "start": "node lib/server/index.js",
    "start.dev": "node -r sucrase/register src/server/index.ts",
    "start.web": "webpack serve --mode development",
    "lint": "eslint . --ext .ts,.tsx",
    "fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "rimraf lib dist",
    "precommit": "npm run lint && npm run test",
    "postinstall": "npm run prepare",
    "test": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn prepare"
    }
  },
  "dependencies": {
    "cleaners": "^0.3.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/styled-components": "^5.1.34",
    "biggystring": "^4.2.3",
    "cleaner-config": "^0.1.10",
    "cors": "^2.8.5",
    "eslint": "^9.32.0",
    "eslint-config-standard-kit": "^1.0.0",
    "express": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "nano": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "qrcode.react": "^4.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "rimraf": "^6.0.1",
    "styled-components": "^6.1.8",
    "sucrase": "^3.35.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
